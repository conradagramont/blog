{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "astro-func-ball": {
      "appLocation": "AstroFuncBall",
      "apiLocation": "AstroFuncBall\\api",
      "outputLocation": ".\\AstroFuncBall\\dist",
      "apiLanguage": "node",
      "apiVersion": "18",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "npm start",
      "run": "npm run dev",
      "appDevserverUrl": "http://localhost:4321"
    }
  }
}